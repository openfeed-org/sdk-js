{
  "name": "sdk-js",
  "version": "0.0.1",
  "description": "JavaScript SDK for Barchart OpenFeed",
  "main": "src/index.ts",
  "repository": "https://github.com/openfeed-org/sdk-js",
  "author": "Dejan Maksimovic <maximovic.d@gmail.com>",
  "private": true,
  "scripts": {
    "build:clear": "rimraf dist",
    "build:ts": "vite build",
    "run:browser": "genversion --es6 -s -d ./generated/version.ts && vite dev",
    "run:node": "genversion --es6 -s -d ./generated/version.ts && ts-node -r tsconfig-paths/register ./src/test.ts",
    "build:ts-check": "tsc -p tsconfig.types.json",
    "generate": "cd node_modules\\proto && ..\\..\\protoc --plugin=..\\..\\node_modules\\.bin\\protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=forceLong=long --ts_proto_out=../../generated *.proto"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.8.1",
    "@types/platform": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "genversion": "^3.1.1",
    "google-protobuf": "^3.21.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "proto": "git+ssh://git@github.com/openfeed-org/proto.git",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.125.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.3"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "isomorphic-ws": "^5.0.0",
    "long": "^5.2.0",
    "platform": "^1.3.6",
    "ws": "^8.8.1"
  }
}